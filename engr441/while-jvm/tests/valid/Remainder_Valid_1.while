int f(int x, int y) {
    return x % y;
}

void main() {
    assert 0 == f(10,5);
    assert 2 == f(10,4);
    assert 1 == f(1,4);
    assert 1 == f(103,2);
    assert 0 == f(-10,5);
    assert -2 == f(-10,4);
    assert -1 == f(-1,4);
    assert -1 == f(-103,2);
    assert 0 == f(-10,-5);
    assert -2 == f(-10,-4);
    assert -1 == f(-1,-4);
    assert -1 == f(-103,-2);
    assert 0 == f(10,-5);
    assert 2 == f(10,-4);
    assert 1 == f(1,-4);
    assert 1 == f(103,-2);
}
