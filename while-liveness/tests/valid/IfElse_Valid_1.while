string f(int x) {
    if(x < 10) {
        return "LESS THAN";
    } else if(x > 10) {
        return "GREATER THAN";
    } else { 
        return "EQUALS";
    }
}

void main() {
    assert "LESS THAN" == f(1);
    assert "EQUALS" == f(10);
    assert "GREATER THAN" == f(11);
    assert "GREATER THAN" == f(1212);
    assert "LESS THAN" == f(-1212);
}
